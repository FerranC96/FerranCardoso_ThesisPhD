@article{maiden_microtubules_2016,
	title = {Microtubules {Inhibit} {E}-{Cadherin} {Adhesive} {Activity} by {Maintaining} {Phosphorylated} p120-{Catenin} in a {Colon} {Carcinoma} {Cell} {Model}},
