@article{joanito_single-cell_2022,
	title = {Single-cell and bulk transcriptome sequencing identifies two epithelial tumor cell states and refines the consensus molecular classification of colorectal cancer},
	volume = {54},
	copyright = {2022 The Author(s)},
	issn = {1546-1718},
	url = {https://www.nature.com/articles/s41588-022-01100-4},
	doi = {10.1038/s41588-022-01100-4},
	abstract = {The consensus molecular subtype (CMS) classification of colorectal cancer is based on bulk transcriptomics. The underlying epithelial cell diversity remains unclear. We analyzed 373,058 single-cell transcriptomes from 63 patients, focusing on 49,155 epithelial cells. We identified a pervasive genetic and transcriptomic dichotomy of malignant cells, based on distinct gene expression, DNA copy number and gene regulatory network. We recapitulated these subtypes in bulk transcriptomes from 3,614 patients. The two intrinsic subtypes, iCMS2 and iCMS3, refine CMS. iCMS3 comprises microsatellite unstable (MSI-H) cancers and one-third of microsatellite-stable (MSS) tumors. iCMS3 MSS cancers are transcriptomically more similar to MSI-H cancers than to other MSS cancers. CMS4 cancers had either iCMS2 or iCMS3 epithelium; the latter had the worst prognosis. We defined the intrinsic epithelial axis of colorectal cancer and propose a refined ‘IMF’ classification with five subtypes, combining intrinsic epithelial subtype (I), microsatellite instability status (M) and fibrosis (F).},
	language = {en},
	number = {7},
	urldate = {2023-06-27},
	journal = {Nature Genetics},
	author = {Joanito, Ignasius and Wirapati, Pratyaksha and Zhao, Nancy and Nawaz, Zahid and Yeo, Grace and Lee, Fiona and Eng, Christine L. P. and Macalinao, Dominique Camat and Kahraman, Merve and Srinivasan, Harini and Lakshmanan, Vairavan and Verbandt, Sara and Tsantoulis, Petros and Gunn, Nicole and Venkatesh, Prasanna Nori and Poh, Zhong Wee and Nahar, Rahul and Oh, Hsueh Ling Janice and Loo, Jia Min and Chia, Shumei and Cheow, Lih Feng and Cheruba, Elsie and Wong, Michael Thomas and Kua, Lindsay and Chua, Clarinda and Nguyen, Andy and Golovan, Justin and Gan, Anna and Lim, Wan-Jun and Guo, Yu Amanda and Yap, Choon Kong and Tay, Brenda and Hong, Yourae and Chong, Dawn Qingqing and Chok, Aik-Yong and Park, Woong-Yang and Han, Shuting and Chang, Mei Huan and Seow-En, Isaac and Fu, Cherylin and Mathew, Ronnie and Toh, Ee-Lin and Hong, Lewis Z. and Skanderup, Anders Jacobsen and DasGupta, Ramanuj and Ong, Chin-Ann Johnny and Lim, Kiat Hon and Tan, Emile K. W. and Koo, Si-Lin and Leow, Wei Qiang and Tejpar, Sabine and Prabhakar, Shyam and Tan, Iain Beehuat},
