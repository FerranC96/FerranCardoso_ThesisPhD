	keywords = {Animals, Cell Proliferation, Intestinal Mucosa, Lgr5+ ISC heterogeneity, Mice, Mice, Transgenic, RNA-Binding Proteins, Receptors, G-Protein-Coupled, Stem Cells, chemotherapy resistance, quiescent stem cell},
