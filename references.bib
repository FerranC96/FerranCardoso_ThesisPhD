	issn = {2045-2322},
	url = {https://www.nature.com/articles/s41598-018-37301-3},
	doi = {10.1038/s41598-018-37301-3},
	abstract = {Colon tumours are hierarchically organized and contain multipotent self-renewing cells, called Cancer Stem Cells (CSCs). We have previously shown that the Notch1 receptor is expressed in Intestinal Stem Cells (ISCs); given the critical role played by Notch signalling in promoting intestinal tumourigenesis, we explored Notch1 expression in tumours. Combining lineage tracing in two tumour models with transcriptomic analyses, we found that Notch1+ tumour cells are undifferentiated, proliferative and capable of indefinite self-renewal and of generating a heterogeneous clonal progeny. Molecularly, the transcriptional signature of Notch1+ tumour cells highly correlates with ISCs, suggestive of their origin from normal crypt cells. Surprisingly, Notch1+ expression labels a subset of CSCs that shows reduced levels of Lgr5, a reported CSCs marker. The existence of distinct stem cell populations within intestinal tumours highlights the necessity of better understanding their hierarchy and behaviour, to identify the correct cellular targets for therapy.},
	language = {en},
	number = {1},
	urldate = {2022-06-27},
	journal = {Scientific Reports},
	author = {Mourao, Larissa and Jacquemin, Guillaume and Huyghe, Mathilde and Nawrocki, Wojciech J. and Menssouri, Naoual and Servant, Nicolas and Fre, Silvia},
	month = jan,
	year = {2019},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Cancer stem cells, Tumour heterogeneity},
	pages = {888},
}

@article{merlos-suarez_intestinal_2011,
	title = {The {Intestinal} {Stem} {Cell} {Signature} {Identifies} {Colorectal} {Cancer} {Stem} {Cells} and {Predicts} {Disease} {Relapse}},
	volume = {8},
	issn = {1934-5909},
	url = {https://www.sciencedirect.com/science/article/pii/S193459091100110X},
	doi = {10.1016/j.stem.2011.02.020},
	abstract = {A frequent complication in colorectal cancer (CRC) is regeneration of the tumor after therapy. Here, we report that a gene signature specific for adult intestinal stem cells (ISCs) predicts disease relapse in CRC patients. ISCs are marked by high expression of the EphB2 receptor, which becomes gradually silenced as cells differentiate. Using EphB2 and the ISC marker Lgr5, we have FACS-purified and profiled mouse ISCs, crypt proliferative progenitors, and late transient amplifying cells to define a gene program specific for normal ISCs. Furthermore, we discovered that ISC-specific genes identify a stem-like cell population positioned at the bottom of tumor structures reminiscent of crypts. EphB2 sorted ISC-like tumor cells display robust tumor-initiating capacity in immunodeficient mice as well as long-term self-renewal potential. Taken together, our data suggest that the ISC program defines a cancer stem cell niche within colorectal tumors and plays a central role in CRC relapse.},
	language = {en},
	number = {5},
	urldate = {2022-06-27},
	journal = {Cell Stem Cell},
	author = {Merlos-Suárez, Anna and Barriga, Francisco M. and Jung, Peter and Iglesias, Mar and Céspedes, María Virtudes and Rossell, David and Sevillano, Marta and Hernando-Momblona, Xavier and da Silva-Diz, Victoria and Muñoz, Purificación and Clevers, Hans and Sancho, Elena and Mangues, Ramón and Batlle, Eduard},
	month = may,
	year = {2011},
	pages = {511--524},
}

@article{zhang_single-cell_2020,
	title = {Single-{Cell} {Analyses} {Inform} {Mechanisms} of {Myeloid}-{Targeted} {Therapies} in {Colon} {Cancer}},
	volume = {181},
	issn = {0092-8674},
	url = {https://www.sciencedirect.com/science/article/pii/S009286742030341X},
	doi = {10.1016/j.cell.2020.03.048},
	abstract = {Single-cell RNA sequencing (scRNA-seq) is a powerful tool for defining cellular diversity in tumors, but its application toward dissecting mechanisms underlying immune-modulating therapies is scarce. We performed scRNA-seq analyses on immune and stromal populations from colorectal cancer patients, identifying specific macrophage and conventional dendritic cell (cDC) subsets as key mediators of cellular cross-talk in the tumor microenvironment. Defining comparable myeloid populations in mouse tumors enabled characterization of their response to myeloid-targeted immunotherapy. Treatment with anti-CSF1R preferentially depleted macrophages with an inflammatory signature but spared macrophage populations that in mouse and human expresses pro-angiogenic/tumorigenic genes. Treatment with a CD40 agonist antibody preferentially activated a cDC population and increased Bhlhe40+ Th1-like cells and CD8+ memory T cells. Our comprehensive analysis of key myeloid subsets in human and mouse identifies critical cellular interactions regulating tumor immunity and defines mechanisms underlying myeloid-targeted immunotherapies currently undergoing clinical testing.},
	language = {en},
	number = {2},
	urldate = {2022-05-26},
	journal = {Cell},
	author = {Zhang, Lei and Li, Ziyi and Skrzypczynska, Katarzyna M. and Fang, Qiao and Zhang, Wei and O’Brien, Sarah A. and He, Yao and Wang, Lynn and Zhang, Qiming and Kim, Aeryon and Gao, Ranran and Orf, Jessica and Wang, Tao and Sawant, Deepali and Kang, Jiajinlong and Bhatt, Dev and Lu, Daniel and Li, Chi-Ming and Rapaport, Aaron S. and Perez, Kristy and Ye, Yingjiang and Wang, Shan and Hu, Xueda and Ren, Xianwen and Ouyang, Wenjun and Shen, Zhanlong and Egen, Jackson G. and Zhang, Zemin and Yu, Xin},
	month = apr,
