@misc{qu_gene_2022,
	title = {Gene {Trajectory} {Inference} for {Single}-cell {Data} by {Optimal} {Transport} {Metrics}},
	copyright = {© 2022, Posted by Cold Spring Harbor Laboratory. The copyright holder for this pre-print is the author. All rights reserved. The material may not be redistributed, re-used or adapted without the author's permission.},
	url = {https://www.biorxiv.org/content/10.1101/2022.07.08.499404v2},
	doi = {10.1101/2022.07.08.499404},
	abstract = {Single-cell sequencing technologies have enabled researchers to study dynamical biological processes, such as cell type differentiation, by inferring cell trajectories. To study the mechanisms underlying a biological process, genes whose expression patterns change smoothly along cell trajectories are identified. However, when a mixture of biological processes coexist in the system, they often cannot be effectively de-mixed using cell trajectory inference approaches. This poses a major challenge to identifying genes that mediate each process and delineating their temporal order. Here, instead of inferring trajectories of cells, we present a new approach, GeneTrajectory, to unravel trajectories of genes that can better resolve distinct biological processes. In our approach, we first compute a cell-cell graph that preserves the manifold structure of cells. Then, we construct a gene-gene graph where the affinities between genes are based on the Wasserstein distances between their distributions on the cell graph. Notably, this method does not require any knowledge of the cell trajectories. We use simulated and two real-world scRNA-seq datasets to demonstrate that GeneTrajectory reveals the gene dynamics underlying complex biological processes that cannot be resolved by commonly-used cell trajectory inference approaches.},
	language = {en},
	urldate = {2022-07-19},
	publisher = {bioRxiv},
	author = {Qu, Rihao and Cheng, Xiuyuan and Myung, Peggy and Sefik, Esen and Flavell, Richard and Kluger, Yuval},
	month = jul,
	year = {2022},
	note = {Pages: 2022.07.08.499404
Section: New Results},
}

@article{burkhardt_mapping_2022,
	title = {Mapping {Phenotypic} {Plasticity} upon the {Cancer} {Cell} {State} {Landscape} {Using} {Manifold} {Learning}},
	issn = {2159-8274},
	url = {https://doi.org/10.1158/2159-8290.CD-21-0282},
	doi = {10.1158/2159-8290.CD-21-0282},
	abstract = {Phenotypic plasticity describes the ability of cancer cells to undergo dynamic, nongenetic cell state changes that amplify cancer heterogeneity to promote metastasis and therapy evasion. Thus, cancer cells occupy a continuous spectrum of phenotypic states connected by trajectories defining dynamic transitions upon a cancer cell state landscape. With technologies proliferating to systematically record molecular mechanisms at single-cell resolution, we illuminate manifold learning techniques as emerging computational tools to effectively model cell state dynamics in a way that mimics our understanding of the cell state landscape. We anticipate that “state-gating” therapies targeting phenotypic plasticity will limit cancer heterogeneity, metastasis, and therapy resistance.Nongenetic mechanisms underlying phenotypic plasticity have emerged as significant drivers of tumor heterogeneity, metastasis, and therapy resistance. Herein, we discuss new experimental and computational techniques to define phenotypic plasticity as a scaffold to guide accelerated progress in uncovering new vulnerabilities for therapeutic exploitation.},
	urldate = {2022-07-19},
	journal = {Cancer Discovery},
	author = {Burkhardt, Daniel B. and San Juan, Beatriz P. and Lock, John G. and Krishnaswamy, Smita and Chaffer, Christine L.},
	month = jun,
	year = {2022},
	pages = {OF1--OF13},
}

@article{barriga_mex3a_2017,
